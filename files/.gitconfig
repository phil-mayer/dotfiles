[user]
    name = Phil Mayer
    email = phil.mayer93@gmail.com

[alias]
    b = branch
    d = diff
    l = log --oneline
    s = status

[core]
    editor = vim
    excludesFile = ~/.gitignore

[credential]
    helper = osxkeychain

[diff]
    mnemonicprefix = true

[init]
    defaultBranch = main

[pull]
    rebase = true

[push]
    default = simple
